/dts-v1/;

#include <vexriscv_incus_soc.dtsi>

/ {
	model = "Incus UP5K EVN";
	compatible = "spinal,incus";

	aliases {
	};

	chosen {
		zephyr,console = &uart0;
		zephyr,shell-uart = &uart0;
	};
};

&gpioA {
	status = "ok";
};


&uart0 {
	status = "ok";
	current-speed = <115200>;
	clock-frequency = <12000000>;
};
