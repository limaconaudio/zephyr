menuconfig GPIO_KENDRYTE
	bool "Kendryte K210 GPIO driver"
	depends on GPIO && SOC_RISCV64_K210
	select HAS_DTS_GPIO
	help
	  Enable driver for the Kendryte K210 GPIO controller.

	  Says n if not sure.

if GPIO_KENDRYTE

config GPIO_KENDRYTE_GPIO_NAME
	string "GPIO driver name"
	default "gpio"

endif # GPIO_KENDRYTE
