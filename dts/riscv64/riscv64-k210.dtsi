/ {
	#address-cells = <1>;
	#size-cells = <1>;
	compatible = "kendryte,k210";
	model = "kendryte,k210";
	cpus {
		#address-cells = <1>;
		#size-cells = <0>;
		cpu@0 {
			clock-frequency = <0>;
			compatible = "kendryte,k210", "riscv";
			device_type = "cpu";
			i-cache-block-size = <64>;
			i-cache-sets = <128>;
			i-cache-size = <16384>;
			reg = <0>;
			riscv,isa = "rv64imac";
			status = "okay";
			timebase-frequency = <32768>;
			hlic: interrupt-controller {
				#interrupt-cells = <1>;
				compatible = "riscv,cpu-intc";
				interrupt-controller;
			};
		};
	};
	soc {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "kendryte,k210","simple-bus";
		ranges;
		clint: clint@2000000 {
			compatible = "riscv,clint0";
			interrupts-extended = <&hlic 3 &hlic 7>;
			reg = <0x2000000 0x10000>;
			reg-names = "control";
		};
		gpio: gpio@50200000 {
			compatible = "kendryte,gpio";
			gpio-controller;
			reg = <0x50200000 0x10000>;
			reg-names = "control";
			#gpio-cells = <2>;
			status = "disabled";
		};
		plic: interrupt-controller@c000000 {
			#interrupt-cells = <1>;
			compatible = "riscv,plic0";
			interrupt-controller;
			interrupts-extended = <&hlic 11>;
			reg = <0xc000000 0x4000000>;
			reg-names = "control";
			riscv,max-priority = <7>;
			riscv,ndev = <52>;
		};
		uart0: serial@50210000 {
			compatible = "kendryte,uart0";
			reg = <0x50210000 0x10000>;
			reg-names = "control";
			label = "uart_0";
			status = "disabled";
		};
		uart1: serial@50220000 {
			compatible = "kendryte,uart0";
			reg = <0x50220000 0x10000>;
			reg-names = "control";
			status = "disabled";
		};
	};
};
